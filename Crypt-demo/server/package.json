{
  "name": "cryptography-server",
  "version": "1.0.0",
  "description": "Backend for cryptography demo",
  "main": "server.js",
  "scripts": {
    "start:server": "nodemon server.js --ignore '../client/'",
    "start:client": "npm run serve --prefix ../client/",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4"
  }
}
